import asyncio
from datatime import datatime
from bot message, bot


bot.on_cmd (ping,
about={header:check how long it takes to ping your userbot
frags: {-a: average ping},
},
group=-1,
)
async def pingme(message: message):
  start= datatime.now
if "-a" in message.flags:
        await message.edit("`!....`")
        await asyncio.sleep(0.3)
        await message.edit("`..!..`")
        await asyncio.sleep(0.3)
